 @model Equipe
@{
    ViewData["Title"] = "Notícias";
}

<form action="@Url.Action("Cadastrar")" method="post" enctype="multipart/form-data">
    <label>ID</label>
    <input type="text" name="IdNotícia" />

    <label>Título</label>
    <input type="text" name="Título" />
    
    <label>Texto</label>
    <input type="text" name="Texto" />

    <label>Imagem</label>
    <input type="file" name="Imagem" />

    <button type="submit">Cadastrar</button>
</form>

<table class="table table-striped table-responsive">
    <thead>
        <th>ID</th>
        <th>Título</th>
        <th>Texto</th>
        <th>Foto</th>
    </thead>
    <tbody>
        @foreach(Notícias n in ViewBag.Notícias){
            <tr>
                <td>@n.IdNoticia</td>
                <td>@n.Titulo</td>
                <td>@n.Texto</td>
                <td>@n.Titulo"</td>
            </tr>
        }
    </tbody>
</table>